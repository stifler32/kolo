namespace _44
{
    class Program
    {
        static void Main(string[] args)
        {
            /*44. Napisać program który pobiera od użytkownika dwie liczby naturalne dodatnie n i k. 
            Następnie rysuje z gwiazdek na ekranie prostokąt posiadający n wierszy i k kolumn. Użyć pętli for.*/
            int n, k;
            Console.WriteLine("Podaj wymiary prostokąta");
            n = Convert.ToInt32(Console.ReadLine());
            k = Convert.ToInt32(Console.ReadLine());
            for (int i = 0; i < n; i++)
            {
                for (int j = 0; j < k; j++)
                    Console.Write("*");
                
                
                Console.WriteLine();
            }
            Console.ReadKey();
        }
    }
}

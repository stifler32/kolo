namespace _39
{
    class   
    {
        static void Main(string[] args)
        {
            /*39. Napisać program który pobiera od użytkownika 5 liczb które umieszcza w tablicy o nazwie tablica. 
            Następnie wypisuje informację ile z nich jest większe od 3. */
            int[] tab = new int[5];
            int l = 0;

            Console.WriteLine("Podaj 5 liczb mości panie");
            for (int i = 0; i < 5; i++)
            {
                tab[i] = Convert.ToInt32(Console.ReadLine());
                if (tab[i] > 3)
                    l++;

            }
            Console.WriteLine("Liczby winksze od czech: {0}",l);
            Console.ReadKey();
        }
    }
    
}

namespace _20
{
    class Program
    {
        static void Main(string[] args)
        {
            //20. Napisać program wypisujący na ekranie konsoli wszystkie liczby naturalne nieparzyste z przedziału [1;100]
            for (int i = 1; i <= 100; i++)
                if(i%2 != 0)
                    Console.Write("{0} ", i);

            Console.ReadKey();
        }
    }
}

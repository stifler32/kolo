namespace _33
{
    class Program
    {
        static void Main(string[] args)
        {
            //33. Napisać program który losuje 10 liczb całkowitych z przedziału [1;10]
            Console.WriteLine("Yo, wypisuje 10 liczb losowych z przedziau 1-10");
            Random rnd = new Random();
            for(int i=0; i<10; i++)
            {
                Console.Write("{0}, ",rnd.Next(1, 11));
            }
            Console.ReadKey();
        }
    }
}

namespace _34
{
    class Program
    {
        static void Main(string[] args)
        {
            /*34.Napisać program który losuje 5 liczb rzeczywistych z przedziału[0; 1]. 
            Liczby są wypisane w jednej linijce, dokładność: 2 miejsca po przecinku.*/

            Console.WriteLine("Yo, wypisuje 10 liczb losowych z przedziau 1-10");
            Random rnd = new Random();
            for (int i = 0; i < 5; i++)
            {
                Console.Write("{0:F2} ",rnd.NextDouble());
            }

            Console.ReadKey();
        }
    }
}
